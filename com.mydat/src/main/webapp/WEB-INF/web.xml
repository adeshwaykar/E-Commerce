<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ResgisterServlet</servlet-name>
  	<display-name>ResgisterServlet</display-name>
  	<description></description>
  	<servlet-class>com.mydata.servlets.ResgisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>loginServlet</servlet-name>
  	<display-name>loginServlet</display-name>
  	<description></description>
  	<servlet-class>com.mydata.servlets.loginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.mydata.servlets.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>productOperationServlet</servlet-name>
  	<display-name>productOperationServlet</display-name>
  	<description></description>
  	<servlet-class>com.mydata.servlets.productOperationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ResgisterServlet</servlet-name>
  	<url-pattern>/ResgisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>loginServlet</servlet-name>
  	<url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>productOperationServlet</servlet-name>
  	<url-pattern>/productOperationServlet</url-pattern>
  </servlet-mapping>
</web-app>
